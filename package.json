    "@ledgerhq/logs": "^6.10.0",
    "@walletconnect/client": "^2.0.0-alpha.26",
    "@walletconnect/qrcode-modal": "^2.0.0-alpha.20",
    "@walletconnect/types": "^2.0.0-beta.26",
    "big.js": "^6.1.1",
    "bn.js": "^5.2.0",
    "buffer": "^6.0.3",
    "core-js": "^3.6.5",
    "is-mobile": "^3.0.0",
    "near-api-js": "0.44.2",
    "ngx-deploy-npm": "^3.1.7",
